@article{aminMechanicalAnalysisMgB22017,
  title = {Mechanical {{Analysis}} of {{MgB2 Based Full Body MRI Coils Under Different Winding Conditions}}},
  author = {Amin, Abdullah A. and Baig, Tanvir N. and Deissler, Robert J. and Sabri, Laith Abed and Doll, David and Tomsic, Michael and Akkus, Ozan and Martens, Michael A.},
  date = {2017-06},
  journaltitle = {IEEE Transactions on Applied Superconductivity},
  shortjournal = {IEEE Trans. Appl. Supercond.},
  volume = {27},
  number = {4},
  pages = {1--5},
  issn = {1558-2515},
  doi = {10.1109/TASC.2017.2654349},
  url = {https://ieeexplore.ieee.org/document/7820118/?arnumber=7820118},
  urldate = {2025-02-10},
  abstract = {The winding of composite superconducting wire around a mandrel is one of the first stages of manufacturing processes of a superconducting magnet. Depending on the method of mechanical support conditions during winding, the strain development at the final stage in a superconducting magnet may vary significantly. Therefore, proper selection of the winding process is important to increase the feasibility for a conduction cooled full body MRI magnet based on magnesium diboride (MgB2), a strain sensitive high-temperature superconductor. A multiscale multiphysics finite element analysis) model of an 18 filament MgB2 wire is developed for strain estimation. The computationally homogenized representative volume element of the composite wire is used in the coil bundle in place of the actual MgB2 wire. The simulation considers winding, thermal cool-down and electromagnetic charging to estimate total strain developed at the final step-electromagnetic charging. Four different types of support conditions are studied and strain development is reported. Results suggest that a combination of radial and axial support at the inner radial surface and outermost axial surfaces of the mandrel, respectively, is the most favorable winding condition with a minimum strain development of 0.021\%, which is half in comparison to no mandrel support.},
  eventtitle = {{{IEEE Transactions}} on {{Applied Superconductivity}}},
  keywords = {ANSYS,Coils,Finite Element Analysis,Magnetic resonance imaging,Magnetomechanical effects,MRI,multiphysics,multiscale modeling,Strain,superconducting coils,Superconducting magnets,Windings,Wires},
  file = {C\:\\Users\\Ali SC1\\Zotero\\storage\\T7JGLU24\\Amin et al. - 2017 - Mechanical Analysis of MgB2 Based Full Body MRI Co.pdf;C\:\\Users\\Ali SC1\\Zotero\\storage\\VFTZL3TA\\7820118.html}
}

@article{aminMultiscaleMultiphysicsModel2016,
