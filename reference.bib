  author = {Amin, Abdullah Al and Baig, Tanvir and Deissler, Robert J and Yao, Zhen and Tomsic, Michael and Doll, David and Akkus, Ozan and Martens, Michael},
  date = {2016-05-01},
  journaltitle = {Superconductor Science and Technology},
