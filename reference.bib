@article{deisslerNumericalSimulationQuench2016,
  title = {Numerical Simulation of Quench Protection for a 1.5 {{T}} Persistent Mode {{MgB2}} Conduction-Cooled {{MRI}} Magnet},
  author = {Deissler, Robert J and Baig, Tanvir and Poole, Charles and Amin, Abdullah and Doll, David and Tomsic, Michael and Martens, Michael},
