  keywords = {Coils,Conductivity,Copper,magnesium diboride,Magnetic resonance imaging,magnetoresistance,Magnetoresistance,magnetothermal conductivity,numerical simulation,quench protection,RRR value,Superconducting magnet,Superconducting magnets,Wires},
